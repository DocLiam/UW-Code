#lang lazy

(require "Lambda.rkt")

(define (SUB1 a)
  ((Y
    (位 (sub1)
      (位 (num)
        (((Car num)
            (If (Z? (Cdr num))
                Z
                (Cons False (Cdr num))))
            (Cons True (sub1 (Cdr num))))))) a))

(define (LENGTH a)
  ((Y
    (位 (length)
      (位 (num len)
        (If (Z? num)
            len
            (length (Cdr num) (ADD1 len)))))) a Z))

